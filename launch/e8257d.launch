<launch>

  <node pkg = "rospkg-ogameasure"
        name = "e8257d"
        type = "e8257d.py">
        <param name="~host" value="192.168.100.37" />
        <param name="~port" value="5025" />
    <remap from = '/dev/e8257d/__IP__/freq_cmd' to = '/dev/e8257d/ip_192_168_100_37/freq_cmd'/>
    <remap from = '/dev/e8257d/__IP__/power_cmd' to = '/dev/e8257d/ip_192_168_100_37/power_cmd'/>
    <remap from = '/dev/e8257d/__IP__/onoff_cmd' to = '/dev/e8257d/ip_192_168_100_37/onoff_cmd'/>
    <remap from = '/dev/e8257d/__IP__/freq' to = '/dev/e8257d/ip_192_168_100_37/freq'/>
    <remap from = '/dev/e8257d/__IP__/power' to = '/dev/e8257d/ip_192_168_100_37/power'/>
    <remap from = '/dev/e8257d/__IP__/onoff' to = '/dev/e8257d/ip_192_168_100_37/onoff'/>

  </node>

</launch>
