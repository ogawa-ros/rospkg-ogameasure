<launch>

  <node pkg = "rospkg-ogameasure"
        name = "ND287"
        type = "ND287.py">

        <param name="~el_usbport" value="/dev/ttyUSB0" />
        <param name="~az_usbport" value="/dev/ttyUSB1" />


    <remap from = '/dev/ND287/__port__/el' to = '/dev/ND287/ttyUSB0/az'/>
    <remap from = '/dev/ND287/__port__/az' to = '/dev/ND287/ttyUSB1/el'/>


  </node>

</launch>
