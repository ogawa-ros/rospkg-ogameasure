<launch>

  <node pkg = "rospkg-ogameasure"
        name = "ma24126a"
        type = "ma24126a.py">

        <param name="~port_list" value="['/dev/ttyACM0','/dev/ttyACM1']" />

        <param name="~ch_num" value="2" />

    <remap from = '/dev/ma24126a/__port__/power/ch1' to = '/dev/ma24126a/ttyACM0/power/ch1'/>
    <remap from = '/dev/ma24126a/__port__/power/ch2' to = '/dev/ma24126a/ttyACM1/power/ch2'/>
    <remap from = '/dev/ma24126a/__port__/zero_set/ch1' to = '/dev/ma24126a/ttyACM0/zero_set/ch1'/>
    <remap from = '/dev/ma24126a/__port__/zero_set/ch2' to = '/dev/ma24126a/ttyACM1/zero_set/ch2'/>

  </node>

</launch>
